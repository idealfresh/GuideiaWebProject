
@{
    ViewData["Title"] = "Magnets";
}


<div class="container product-container">
    <div class="row">
        <div class="col">
            <a class="card-link" asp-area="" asp-controller="NeoMagnet" asp-action="Magnet">
                <div class="card-body" id="magnet-image">
                </div>
                <div class="card-footer">
                    <small class="text-muted">Неодимови магнити</small>
                </div>
            </a>
        </div>
        <div class="col">          
                <a class="card-link" asp-area="" asp-controller="NeoMagnet" asp-action="MagneticBall">
                    <div class="card-body" id="magneticBall-image">
                    </div>
                    <div class="card-footer">
                        <small class="text-muted">Магнитни топчета - Magnetic Ball</small>
                    </div>
                </a>
            </div>

        </div>
</div>

